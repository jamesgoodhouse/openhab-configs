        Group Alarm "Alarm" <alarm> (Room_Closet_Bedroom) ["AlarmSystem"]
    Group Room_Laundry "Laundry Room" <washingmachine> (Floor_Second) ["LaundryRoom"]
    Group Room_Media "Media Room" <microphone> (Floor_Second) ["Room"]
    Group Room_Hallway "Hallway" <corridor> (Floor_Second) ["Corridor"]

Group Outdoor "Outdoor" <garden> ["Outdoor"]
  Group AQI_Home "Air Quality" <colorlight> (Outdoor) ["Sensor"]
  Group HVAC_OutdoorUnit "Condenser" <heating> (Outdoor) ["HVAC"]








