    Group Room_Dining "Dining Room" (Floor_First) ["DiningRoom"]
    Group Room_Kitchen "Kitchen" <kitchen> (Floor_First) ["Kitchen"]
    Group Room_Living "Living Room" (Floor_First) ["LivingRoom"]
